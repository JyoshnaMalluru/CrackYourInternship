class Solution {
    // Function to find the smallest positive number missing from the array.
    public int missingNumber(int[] arr) {
       
        HashSet<Integer> set = new HashSet<>();
        for(int i=0;i<arr.length;i++){
            set.add(arr[i]);
        }
        int i=1;
        while(set.contains(i)){
            i++;
        }
        return i;
    }
}
